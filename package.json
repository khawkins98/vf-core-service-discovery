{
  "name": "vf-core-service-discovery",
  "version": "0.1.0-alpha.0",
  "description": "A tool to analyse the usage of vf-core in your project",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "vf-core-service-discovery": "dist/cli.js"
  },
  "scripts": {
    "start": "yarn clean:dist && tsc --project ./ --watch",
    "build": "yarn clean:dist && tsc --project ./",
    "clean:dist": "rimraf dist",
    "test": "nyc ava",
    "prepublish": "yarn build"
  },
  "author": "Ossama Edbali <ossedb@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oss6/vf-core-service-discovery/issues",
    "email": "ossedb@gmail.com"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.8",
    "@types/rimraf": "^3.0.0",
    "@types/yargs": "^15.0.11",
    "@types/yarnpkg__lockfile": "^1.1.4",
    "ava": "^3.13.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@yarnpkg/lockfile": "^1.1.0",
    "boxen": "^4.2.0",
    "chalk": "^4.1.0",
    "date-fns": "^2.16.1",
    "glob": "^7.1.6",
    "isomorphic-git": "^1.8.0",
    "lodash": "^4.17.20",
    "winston": "^3.3.3",
    "yaml": "^1.10.0",
    "yargs": "^16.1.1"
  },
  "ava": {
    "files": [
      "test/**",
      "!test/fixture/**"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
